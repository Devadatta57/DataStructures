package ArraysProgramsSearching;

import java.util.Arrays;

class BinaryS{
	public static int binary(int[] a,int key){
		int low=a[0];
		int high=a[a.length]-1;
		int mid=(low+high)/2;
		while(low<=high) {
		if(a[mid]==key) {
			return mid;
		}
		else if(a[mid]<key){
			low=mid+1;
		}
		else {
			high=mid-1;
		}
		
		}
		return -1;
	}
}
public class BinarySearch {
	public static void main(String[] args) {
	int[] a=new int[6];
	a=new int[] {12,43,125,34,89,45};
	
       Arrays.sort(a);
       BinaryS.binary(a,45);
	
	}
}
