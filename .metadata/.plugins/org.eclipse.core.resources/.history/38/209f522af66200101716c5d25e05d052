package ArraysPrograms;

import java.util.Arrays;

class ArraySHAdj{
	public static void smallHigh(int[] a,int low,int high) {
		for(int i=0;i<=a.length;i++) {
		if(low<=high) {
			System.out.print(a[low]+" ");
			low++;
			System.out.print(a[high]+" ");
			high--;
		}
	}
}
}
public class ArraySmallHighestAdj {
	public static void main(String[] args) {
		 int a[]=new int[5];
	     a=new int[] {1,2,3,4,5};
	     
	     Arrays.sort(a);
	     
//	     for(int i=0;i<a.length;i++) {
//	    	 System.out.println(a[i]);
//	     }
	  
	     ArraySHAdj.smallHigh(a,0,a.length-1);
	}
}
